{
    "name": "viget/craft-sidebar-toggle",
    "description": "Add the ability to toggle the sidebar",
    "type": "craft-plugin",
    "version": "3.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "sidebar toggle"
    ],
    "support": {
        "docs": "https://github.com/vigetlabs/craft-sidebartoggle/blob/master/README.md",
        "issues": "https://github.com/vigetlabs/craft-sidebartoggle/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Trevor Davis",
            "homepage": "https://www.viget.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
          "viget\\sidebartoggle\\": "src/"
        }
    },
    "extra": {
        "name": "Sidebar Toggle",
        "handle": "sidebar-toggle",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/vigetlabs/craft-sidebartoggle/master/CHANGELOG.md",
        "class": "viget\\sidebartoggle\\SidebarToggle"
    }
}
